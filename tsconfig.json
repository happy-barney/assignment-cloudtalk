{
	"compilerOptions": {
		"resolveJsonModule": true,
		"incremental": false,
		"target": "ES2020",
		"module": "commonjs",
		"outDir": "./build",
		"strict": true,
		"typeRoots": ["./node_modules/@types", "./lib/types"],
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": [
		"./lib",
		"./t",
	],
}
